<div class="container">
  <div class="content-block border-bottom border-secondary" data-aos="fade">
    <div class="container container-md mb-8 text-center">
      <%= inline_svg("icons/key-stakeholder.svg", class: "isvg") %>
      <h2 class="title mt-3">Key Stakeholders</h2>
      <p>
        Those who have pledged on Open Collective <b>$750</b> or more a month to Solidus.
      </p>
    </div>
    <div class="community-logos">
      <div class="row align-items-center justify-content-center">
        <div class="col-6 col-sm-4 col-lg-auto">
          <a class="item" href="https://opencollective.com/nebulab" target="_blank"><%= inline_svg("community-logos/nebulab.svg", class: "isvg") %></a>
        </div>
        <div class="col-6 col-sm-4 col-lg-auto">
          <a class="item" href="https://opencollective.com/engine-commerce" target="_blank"><%= inline_svg("community-logos/engine.svg", class: "isvg") %></a>
        </div>
        <div class="col-6 col-sm-4 col-lg-auto">
          <a class="item" href="https://opencollective.com/supergoodsoft" target="_blank"><%= inline_svg("community-logos/super-good.svg", class: "isvg") %></a>
        </div>
      </div>
    </div>
  </div>
  
  <div class="content-block border-bottom border-secondary" data-aos="fade">
    <div class="container container-md mb-8 text-center">
      <%= inline_svg("icons/sponsor.svg", class: "isvg") %>
      <h3 class="title mt-3">Sponsors</h3>
      <p>
        Those who have pledged on Open Collective from <b>$100</b> to <b>$500</b> a month to Solidus.
      </p>
    </div>
    <div class="community-logos">
      <div class="row align-items-center justify-content-center">
        <div class="col-6 col-sm-4 col-lg-auto">
          <a class="item" href="https://opencollective.com/boomerdigital1" target="_blank"><%= inline_svg("community-logos/boomer-digital.svg", class: "isvg") %></a>
        </div>
        <div class="col-6 col-sm-4 col-lg-auto">
          <a class="item" href="https://opencollective.com/pberkenbosch" target="_blank"><img class="img-fluid rounded-circle" src="<%= image_path('community-logos/p-berkenbosch.png') %>" srcset="<%= image_path('community-logos/p-berkenbosch.png') %>, <%= image_path('community-logos/p-berkenbosch@2x.png') %> 2x" alt="Peter berkenbosch"></a>
        </div>
        <div class="col-6 col-sm-4 col-lg-auto">
          <a class="item" href="https://opencollective.com/modded-euros" target="_blank"><%= inline_svg("community-logos/moddedeuros.svg", class: "isvg") %></a>
        </div>
      </div>
    </div>
  </div>
  
  <div class="content-block border-bottom border-secondary" data-aos="fade">
    <div class="container container-md mb-8 text-center">
      <%= inline_svg("icons/backer.svg", class: "isvg") %>
      <h3 class="title mt-3">Backers</h3>
      <p>
        Those who have pledged on Open Collective from <b>$10</b> to <b>$50</b> a month to Solidus.
      </p>
    </div>
    <div class="community-logos">
      <div class="row align-items-center justify-content-center">
        <div class="col-3 col-sm-2 col-lg-auto">
          <a class="item" href="https://opencollective.com/tcwequipment1" target="_blank"><%= inline_svg("community-logos/twc.svg", class: "isvg") %></a>
        </div>
        <div class="col-3 col-sm-2 col-lg-auto">
          <a class="item" href="https://opencollective.com/sean-denny" target="_blank"><img class="img-fluid rounded-circle" src="<%= image_path('community-logos/s-denny.png') %>" srcset="<%= image_path('community-logos/s-denny.png') %>, <%= image_path('community-logos/s-denny@2x.png') %> 2x" alt="Sean Denny"></a>
        </div>
        <div class="col-3 col-sm-2 col-lg-auto">
          <a class="item" href="https://opencollective.com/jacob-herrington" target="_blank"><img class="img-fluid rounded-circle" src="<%= image_path('community-logos/jacob-herrington.png') %>" srcset="<%= image_path('community-logos/jacob-herrington.png') %>, <%= image_path('community-logos/jacob-herrington@2x.png') %> 2x" alt="Jacob Herrington"></a>
        </div>
        <div class="col-3 col-sm-2 col-lg-auto">
          <a class="item" href="https://opencollective.com/b_puryear" target="_blank"><img class="img-fluid rounded-circle" src="<%= image_path('community-logos/b-puryear.png') %>" srcset="<%= image_path('community-logos/b-puryear.png') %>, <%= image_path('community-logos/b-puryear@2x.png') %> 2x" alt="Blake Puryear"></a>
        </div>
        <div class="col-3 col-sm-2 col-lg-auto">
          <a class="item" href="https://opencollective.com/vzqzac" target="_blank"><img class="img-fluid rounded-circle" src="<%= image_path('community-logos/o-vazquez.png') %>" srcset="<%= image_path('community-logos/o-vazquez.png') %>, <%= image_path('community-logos/o-vazquez@2x.png') %> 2x" alt="Omar Vazquez"></a>
        </div>
        <div class="col-3 col-sm-2 col-lg-auto">
            <a class="item" href="https://opencollective.com/rulocontreras_" target="_blank"><img class="img-fluid rounded-circle" src="<%= image_path('community-logos/r-contreras.png') %>" srcset="<%= image_path('community-logos/r-contreras.png') %>, <%= image_path('community-logos/r-contreras@2x.png') %> 2x" alt="Raul Contreras"></a>
        </div>
      </div>
    </div>
  </div>
  
</div>